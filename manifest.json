{
    "manifest_version": 3,
    "name": "Snipzo",
    "version": "1.0",
    "description": "Free Keybind to Snippet: Snipzo",
    "icons": {
      "48": "icons/snipzo_48.png",
      "128": "icons/snipzo_128.png"
    },
    "permissions": [
      "storage"
    ],
    "host_permissions": [
      "<all_urls>"
    ],
    "options_ui": {
        "page": "options.html",
        "open_in_tab": false
    },
    "action": {
      "default_icon": {
        "48": "icons/snipzo_48.png",
        "128": "icons/snipzo_128.png"
      },
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content-script.js"]
      }
    ]
  }
  
